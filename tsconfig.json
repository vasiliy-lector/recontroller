{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "lib": [
            "esnext"
        ],
        "outDir": "./lib/",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "jsx": "react"
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
    ]
}
